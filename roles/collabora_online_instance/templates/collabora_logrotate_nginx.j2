{{ www_log | mandatory }}/{{ collabora_domain }}/*.log {
        weekly
        missingok
        rotate 54
        compress
        delaycompress
        notifempty
	    create 0640 www-data adm
        sharedscripts
        prerotate
                if [ -d /etc/logrotate.d/httpd-prerotate ]; then \
                        run-parts /etc/logrotate.d/httpd-prerotate; \
                fi \
        endscript
        postrotate
                invoke-rc.d nginx rotate >/dev/null 2>&1
        endscript
}
